<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="urn:no:nav:maskinelletrekk:trekk:v1"
            xmlns="urn:no:nav:maskinelletrekk:trekk:v1"
            attributeFormDefault="qualified"
            elementFormDefault="qualified">

    <xsd:element name="trekk">
        <xsd:complexType>
            <xsd:choice>
                <xsd:element name="trekkRequest" type="TrekkRequest" maxOccurs="1000"/>
                <xsd:element name="trekkResponse" type="TrekkResponse" maxOccurs="1000"/>
            </xsd:choice>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="TrekkRequest">
        <xsd:sequence>
            <xsd:element name="bruker" type="xsd:string"/>
            <xsd:element name="periode" type="Periode"/>
            <xsd:element name="trekkvedtakId" type="xsd:int"/>
            <xsd:element name="oppdragsvedtak" type="Oppdragsvedtak" minOccurs="0" maxOccurs="1000"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TrekkResponse">
        <xsd:sequence>
            <xsd:element name="trekkvedtakId" type="xsd:int"/>
            <xsd:element name="beslutning" type="Beslutning"/>
            <xsd:element name="totalSatsOS" type="xsd:decimal" />
            <xsd:element name="totalSatsArena" type="xsd:decimal" />
            <xsd:element name="vedtak" type="Vedtak" minOccurs="0" maxOccurs="1000"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Periode">
        <xsd:sequence>
            <xsd:element name="fom" type="xsd:date"/>
            <xsd:element name="tom" type="xsd:date"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Oppdragsvedtak">
        <xsd:sequence>
            <xsd:element name="sats" type="xsd:decimal"/>
            <xsd:element name="satstype" type="Satstype"/>
            <xsd:element name="periode" type="Periode"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Vedtak">
        <xsd:sequence>
            <xsd:element name="vedtaksperiode" type="Periode"/>
            <xsd:element name="tema" type="xsd:string"/>
            <xsd:element name="rettighetType" type="xsd:string"/>
            <xsd:element name="dagsats" type="xsd:decimal" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="Satstype">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="DAG"/>
            <xsd:enumeration value="MND"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Beslutning">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="OS"/>
            <xsd:enumeration value="ABETAL"/>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>